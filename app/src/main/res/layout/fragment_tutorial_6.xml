<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.tutorial_6">

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:id="@+id/textView14" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/kleverson_nascimento"
        android:id="@+id/imageView_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleType="fitXY" />

    <Button
        android:text="Ir para home"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        style="@android:style/Widget.Material.Light.Button.Inset"
        android:onClick="vou_home"
        android:elevation="24dp"
        app:layout_constraintRight_toRightOf="@+id/textView14"
        app:layout_constraintLeft_toLeftOf="@+id/textView14"
        android:layout_width="200dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView14"
        android:layout_marginBottom="30dp" />

</android.support.constraint.ConstraintLayout>
